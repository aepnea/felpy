<h1>Listing orders</h1>

<table>
  <thead>
    <tr>
      <th>Product</th>
      <th>Description</th>
      <th>Attachment</th>
      <th>User</th>
      <th>Phone</th>
      <th>Street</th>
      <th>Number</th>
      <th>Depto</th>
      <th>Delivery name</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.product_id %></td>
        <td><%= order.description %></td>
        <td><%= order.attachment %></td>
        <td><%= order.user_id %></td>
        <td><%= order.phone %></td>
        <td><%= order.street %></td>
        <td><%= order.number %></td>
        <td><%= order.depto %></td>
        <td><%= order.delivery_name %></td>
        <td><%= order.price_id %></td>
        <td><%= link_to 'Show', order %></td>
        <td><%= link_to 'Edit', edit_order_path(order) %></td>
        <td><%= link_to 'Destroy', order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order', new_order_path %>
